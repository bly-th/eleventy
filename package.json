{
  "name": "@bly-th/eleventy",
  "version": "2.0.0",
  "description": "An eleventy starter framework.",
  "repository": {
    "type": "git",
    "url": "https://github.com/bly-th/eleventy"
  },
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:css": "NODE_ENV=production npx @tailwindcss/cli -i src/_includes/css/app.css -o src/css/main.css --watch",
    "dev:11ty": "eleventy --serve --incremental",
    "production": "npx eleventy"
  },
  "blyth": {
    "utilityOutputPath": "src/_includes/css/utility"
  },
  "type": "module",
  "author": "Ryan Gittings",
  "license": "GPL-2.0 AND MIT",
  "homepage": "https://github.com/bly-th/eleventy",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^6.0.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^2.0.2",
    "@bly-th/css": "^1.0.5",
    "@tailwindcss/cli": "^4.0.0",
    "npm-run-all": "^2.1.0",
    "tailwindcss": "^4.0.0"
  }
}
